<h1>Ranting form</h1>




<form action = "find_date" method = "POST" id = "renting_info_form">
    
    <label id = "user_id" name = "user_id">User: {{user_id}}</label><br>

    <label id = "car_id" name = "car_id">Car: {{car_id}}</label><br>

    
    <label>startDate</label>
    <input booked_date = {{booked_date_arr}} type="text" id = "start_date" class = "date_input" name = "start_date" min = {{min_date}}>

    <label>endDate</label>
    <input type="text" id = "end_date" class = "date_input" name = "end_date" disabled>

    <input type = "submit" value = "Submit" class = "Submit">
</form>


{{#if error_flag}}
    <p class = "error_info">{{message}}</p>
{{/if}}


